{
  "name": "WebAP.to",
  "short_name": "WebAP",
  "description": "One-click Fediverse interactions from anywhere",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#6366f1",
  "icons": [
    {
      "src": "/images/webap.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "/images/webap.webp",
      "sizes": "512x512",
      "type": "image/webp",
      "purpose": "any maskable"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+ap",
      "url": "/handle.html?uri=%s"
    }
  ],
  "categories": ["social", "utilities"],
  "orientation": "any"
}
